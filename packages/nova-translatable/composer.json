{
    "name": "packages/nova-translatable",
    "description": "A Laravel Nova field.",
    "version": "0.0.1",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Packages\\Translatable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Packages\\Translatable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
