{
    "name": "packages/approval",
    "description": "A CMS Approval Flow",
    "version": "0.0.1",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/nova": "4.33.1"
    },
    "autoload": {
        "psr-4": {
            "Packages\\Approval\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Packages\\Approval\\ApprovalServiceProvider",
                "Packages\\Approval\\ApprovalNovaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
