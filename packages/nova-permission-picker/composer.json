{
  "name": "packages/nova-permission-picker",
  "description": "A Laravel Nova field.",
  "version": "0.0.1",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "Packages\\PermissionPicker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Packages\\PermissionPicker\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
